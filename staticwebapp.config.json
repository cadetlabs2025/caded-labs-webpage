{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["*.{css,scss,js,png,gif,ico,jpg,jpeg,svg,pdf}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".pdf": "application/pdf"
  }
}
